<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yun.opern.dao.UpdateInfoDao">

    <select id="getLastUpdateInfo" resultType="com.yun.opern.model.update.UpdateInfo">
        SELECT *
        FROM tbl_updateinfo
        ORDER BY updateDataTime DESC
        LIMIT 1
    </select>

</mapper>