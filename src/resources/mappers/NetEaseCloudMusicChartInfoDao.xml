<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yun.opern.dao.NetEaseCloudMusicChartInfoDao">

    <select id="musicChart" resultType="com.yun.opern.model.netease.NetEaseCloudMusicChartInfo">
        SELECT * from tbl_neteasemusicchartinfo ORDER BY sort ASC
    </select>

    <select id="musicChartMusic" resultType="com.yun.opern.model.netease.NetEaseCloudMusicChartMusicInfo">
        SELECT * from tbl_neteasemusicchartmusicinfo WHERE chartId = #{chartId} ORDER BY sort ASC
    </select>

</mapper>