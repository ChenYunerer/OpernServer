<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yun.opern.dao.OpernImgInfoDao">
    <select id="getOpernImgInfo" resultType="com.yun.opern.model.OpernImgInfo">
        SELECT
            *
        FROM
            tbl_opernimginfo
        WHERE
            opernId = #{0}
        ORDER BY
            opernIndex ASC
    </select>

</mapper>